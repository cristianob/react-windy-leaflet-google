{
  "name": "react-windy-leaflet-google",
  "version": "0.1.1",
  "description": "A port of react-leaflet-google to react-windy-leaflet",
  "main": "dist/react-leaflet-google.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean:lib": "rm -Rf ./lib",
    "compile:lib": "babel src --out-dir lib",
    "build:lib": "npm run compile:lib",
    "build:dist": "webpack src/index.js dist/react-leaflet-google.js & webpack -p src/index.js dist/react-leaflet-google.min.js",
    "build": "npm run build:lib && npm run build:dist",
    "example": "webpack-dev-server --config ./example/webpack.config.js",
    "lint": "eslint ./src",
    "release:major": "npm version major && git push --follow-tags",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:patch": "npm version patch && git push --follow-tags",
    "publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:cristianob/react-windy-leaflet-google.git"
  },
  "keywords": [
    "react",
    "leaflet",
    "google",
    "windy"
  ],
  "author": "Cristiano Costa <cristiano@cristianocosta.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cristianob/react-windy-leaflet-google/issues"
  },
  "homepage": "https://github.com/cristianob/react-windy-leaflet-google#readme",
  "peerDependencies": {
    "google-maps": "^3.2.1",
    "leaflet": "1.4.0",
    "prop-types": "^15.5.0",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "react-windy-leaflet": "^0.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.1.18",
    "eslint": "^3.8.1",
    "eslint-plugin-react": "^6.4.1",
    "google-maps": "^3.3.0",
    "leaflet": "1.4.0",
    "lodash.isequal": "^4.4.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-windy-leaflet": "^0.1.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.12.1"
  }
}
